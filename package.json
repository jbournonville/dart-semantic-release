{
  "name": "dart-semantic-release",
  "version": "1.0.0",
  "description": "A semantic-release plugin for dart package",
  "main": "index.js",
  "repository": "https://github.com/jbournonville/dart-semantic-release.git",
  "author": "Julien BOURNONVILLE <julien.bournonville@gmail.com>",
  "keywords": [
    "semantic-release",
    "dart"
  ],
  "dependencies": {
    "@semantic-release/error": "^4.0.0",
    "@semantic-release/git": "^10.0.1",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "semantic-release": "^22.0.12"
  },
  "peerDependencies": {
    "prettier": "3.1.1",
    "semantic-release": ">=20.1.0"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "engines": {
    "node": "^18.17 || >=20"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "prettier --check \"{lib,test}/**/*.{js,json,ts}\" \"*.{md,json,js}\" \".github/**/*.yml\"",
    "lint:fix": "prettier --write \"{lib,test}/**/*.{js,json,ts}\" \"*.{md,json,js}\" \".github/**/*.yml\""
  },
  "type": "module"
}
